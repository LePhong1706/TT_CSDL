@model Web_vuottai.Models.THOIKHOABIEU

@{
    ViewData["Title"] = "Xóa";
}

<h1>Xóa năm học</h1>

<h3>Bạn có chắc chắn muốn xóa mục này?</h3>
<div>
    <h4>Năm học - Học kỳ</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NamHoc)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NamHoc)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HocKy)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HocKy)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TenTkb)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TenTkb)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="TkbId" />
        <input type="submit" value="Xóa" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-secondary">Quay lại</a>
    </form>
</div>
